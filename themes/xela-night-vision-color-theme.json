  "tokenColors": [
    { "scope": ["source","meta.brace","meta.delimiter","punctuation","punctuation.definition.parameters","punctuation.definition.block"], "settings": { "foreground": "#F1FFF1" } },
    { "scope": ["comment","punctuation.definition.comment"], "settings": { "foreground": "#8EA38E" } },
    { "scope": ["comment.block.documentation","comment.line.documentation"], "settings": { "foreground": "#97AD97" } },
    { "scope": ["keyword.other.todo","keyword.other.fixme","keyword.other.xxx"], "settings": { "foreground": "#FFD166", "fontStyle": "bold" } },
    { "scope": ["keyword","keyword.control","storage.type","storage.modifier","keyword.operator.new","keyword.operator.expression"], "settings": { "foreground": "#A8FF74", "fontStyle": "bold" } },
    { "scope": ["entity.name.function","variable.function","meta.function-call","support.function.any-method"], "settings": { "foreground": "#6BFF3E" } },
    { "scope": ["string","string.quoted","string.template"], "settings": { "foreground": "#79E49A" } },
    { "scope": ["constant.numeric","variable.other.constant","entity.name.constant","support.constant"], "settings": { "foreground": "#EAF7C6" } },
    { "scope": ["constant.language.boolean","constant.boolean"], "settings": { "foreground": "#79E49A", "fontStyle": "bold" } },
    { "scope": ["constant.language.null","constant.language.undefined","constant.language.nil","constant.language.none"], "settings": { "foreground": "#A9BDA9" } },
