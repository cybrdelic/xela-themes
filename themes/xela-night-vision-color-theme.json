    { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#EAF7C6" } },
    { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#9DB39D" } },
    { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#8EA38E" } },
    { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#9DB39D", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#A8FF74" } },
    { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#192419" } },
    { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["markup.table"], "settings": { "foreground": "#E4FFE4" } },
    { "scope": ["markup.table.header"], "settings": { "foreground": "#C6F7C6", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#192419" } },
    { "scope": ["markup.strikethrough"], "settings": { "foreground": "#A9BDA9", "fontStyle": "strikethrough" } },
    { "scope": ["markup.inserted"], "settings": { "foreground": "#79E49A" } },
    { "scope": ["markup.deleted"], "settings": { "foreground": "#FF6B6B" } },
    { "scope": ["markup.changed"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#EAF7C6" } },
    { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#A8FF74" } },
    { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#A8FF74" } }
