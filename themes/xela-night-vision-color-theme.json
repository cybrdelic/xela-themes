    { "scope": ["meta.decorator","entity.name.function.decorator","storage.type.annotation","meta.annotation"], "settings": { "foreground": "#C6F7C6" } },
    { "scope": ["variable.parameter","meta.parameter","meta.function.parameters"], "settings": { "foreground": "#EAF7C6" } },
    { "scope": ["variable.other.property","support.variable.property","meta.object-literal.key","meta.mapping.key","string.unquoted.label.js"], "settings": { "foreground": "#F1FFF1" } },
    { "scope": ["keyword.control.import","keyword.control.export","support.module","entity.name.namespace"], "settings": { "foreground": "#A8FF74" } },
    { "scope": ["support.class.component","entity.name.tag.tsx","entity.name.tag.jsx"], "settings": { "foreground": "#C6F7C6" } },
    { "scope": ["entity.name.tag.html","entity.name.tag"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["punctuation.definition.tag","punctuation.section.embedded.begin.tsx","punctuation.section.embedded.end.tsx"], "settings": { "foreground": "#9DB39D" } },
    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#EAF7C6" } },
    { "scope": ["variable.language.self","variable.language.this"], "settings": { "foreground": "#A9BDA9" } },
    { "scope": ["invalid.deprecated","invalid.illegal"], "settings": { "foreground": "#FFE9EF" } },

    { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
    { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#EAF7C6", "fontStyle": "bold" } },
    { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#A8FF74", "fontStyle": "bold" } },
    { "scope": ["heading.3.markdown"], "settings": { "foreground": "#79E49A", "fontStyle": "bold" } },
    { "scope": ["heading.4.markdown"], "settings": { "foreground": "#C6F7C6", "fontStyle": "bold" } },
    { "scope": ["heading.5.markdown"], "settings": { "foreground": "#90D4A6", "fontStyle": "bold" } },
    { "scope": ["heading.6.markdown"], "settings": { "foreground": "#E4FFE4", "fontStyle": "bold" } },

    { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#F6FFF6", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#A8FF74" } },
    { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#EAF7C6", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#A8FF7488" } },
    { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link"], "settings": { "foreground": "#A8FF74", "fontStyle": "underline" } },
