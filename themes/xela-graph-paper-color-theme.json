{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Graph Paper â€” Light Grid",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#FAFAFA",
    "editor.foreground": "#1F2937",
    "editorLineNumber.foreground": "#9CA3AF",
    "editor.lineHighlightBackground": "#00000008",
    "editor.selectionBackground": "#2563EB22",
    "editor.inactiveSelectionBackground": "#2563EB11",

    "editorCursor.foreground": "#2563EB",

  "editorIndentGuide.background1": "#00000012",
  "editorIndentGuide.activeBackground1": "#00000026",
  "editorIndentGuide.background2": "#00000010",
  "editorIndentGuide.activeBackground2": "#00000022",
    "editorRuler.foreground": "#0000001A",
    "editorWhitespace.foreground": "#0000001A",

    "editorWidget.background": "#FFFFFFF2",
    "editorWidget.border": "#E5E7EB",
    "editorHoverWidget.background": "#FFFFFFF2",
    "editorHoverWidget.border": "#E5E7EB",

    "sideBar.background": "#FFFFFF",
    "sideBar.foreground": "#111827",
    "sideBar.border": "#E5E7EB",

    "activityBar.background": "#FFFFFF",
    "activityBar.foreground": "#111827",
    "activityBar.border": "#E5E7EB",

    "titleBar.activeBackground": "#FFFFFF",
    "titleBar.inactiveBackground": "#FFFFFF",
    "titleBar.activeForeground": "#111827",
    "titleBar.inactiveForeground": "#6B7280",
    "titleBar.border": "#E5E7EB",

    "panel.background": "#FFFFFF",
    "panel.border": "#E5E7EB",

    "tab.activeBackground": "#FFFFFF",
    "tab.inactiveBackground": "#F9FAFB",
    "tab.border": "#E5E7EB",
    "tab.activeForeground": "#111827",
    "tab.inactiveForeground": "#6B7280",

  "list.activeSelectionBackground": "#EEF2FF",
  "list.inactiveSelectionBackground": "#F3F4F6",
  "list.hoverBackground": "#F3F4F6",
  "list.focusBackground": "#E5E7EB",
  "tree.indentGuidesStroke": "#0000001A",

    "textLink.foreground": "#2563EB",
  "textBlockQuote.background": "#F3F4F6",
    "textBlockQuote.border": "#E5E7EB",
    "textPreformat.foreground": "#111827",
  "textSeparator.foreground": "#D1D5DB",
  "textCodeBlock.background": "#0000000D",

    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#111827",
    "terminal.ansiBlack": "#111827",
    "terminal.ansiRed": "#DC2626",
    "terminal.ansiGreen": "#16A34A",
    "terminal.ansiYellow": "#CA8A04",
    "terminal.ansiBlue": "#2563EB",
    "terminal.ansiMagenta": "#7C3AED",
    "terminal.ansiCyan": "#0891B2",
    "terminal.ansiWhite": "#F3F4F6",
    "terminal.ansiBrightBlack": "#374151",
    "terminal.ansiBrightRed": "#EF4444",
    "terminal.ansiBrightGreen": "#22C55E",
    "terminal.ansiBrightYellow": "#F59E0B",
    "terminal.ansiBrightBlue": "#3B82F6",
    "terminal.ansiBrightMagenta": "#8B5CF6",
    "terminal.ansiBrightCyan": "#06B6D4",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
  { "scope": ["markup.fenced_code", "markup.inline.raw"], "settings": { "foreground": "#111827" } },
  { "scope": ["markup.fenced_code.block"], "settings": { "foreground": "#111827" } },
    { "scope": ["comment"], "settings": { "foreground": "#6B7280", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage.type"], "settings": { "foreground": "#111827", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#1F2937" } },
    { "scope": ["string"], "settings": { "foreground": "#065F46" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#7C3AED" } },
    { "scope": ["markup.bold"], "settings": { "fontStyle": "bold" } },
    { "scope": ["markup.italic"], "settings": { "fontStyle": "italic" } },
    { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "foreground": "#111827", "fontStyle": "bold" } },
    { "scope": ["markup.list"], "settings": { "foreground": "#111827" } },
  { "scope": ["markup.inline.raw"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#111827", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#2563EB", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#7C3AED", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#0891B2", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#065F46", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#6B7280", "fontStyle": "bold" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#2563EB", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#7C3AED" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#6B7280" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#E5E7EB" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#4B5563", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#2563EB" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#E5E7EB" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#2563EB" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#111827" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#0F172A", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#E5E7EB" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#9CA3AF" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#16A34A" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#DC2626" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#F59E0B" } },
  { "scope": ["markup.strikethrough"], "settings": { "foreground": "#6B7280", "fontStyle": "strikethrough" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#2563EB" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#2563EB" } }
  ],
  "semanticTokenColors": {
    "comment": "#6B7280",
    "keyword": "#111827",
    "function": "#1F2937",
    "string": "#065F46",
    "number": "#7C3AED",
    "type": "#0F172A"
  }
}
