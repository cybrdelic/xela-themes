    { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#98B8A7" } },
    { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#8FA79A" } },
    { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#98B8A7", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#47F29C" } },
    { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#113123" } },
    { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["markup.table"], "settings": { "foreground": "#DAFFE9" } },
    { "scope": ["markup.table.header"], "settings": { "foreground": "#6DE6B2", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#113123" } },
    { "scope": ["markup.strikethrough"], "settings": { "foreground": "#A1C1B1", "fontStyle": "strikethrough" } },
    { "scope": ["markup.inserted"], "settings": { "foreground": "#79E49A" } },
    { "scope": ["markup.deleted"], "settings": { "foreground": "#FF6B6B" } },
    { "scope": ["markup.changed"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#E0F4E9" } },
    { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#47F29C" } },
    { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#47F29C" } }
