    { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#47F29C" } },
    { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#E0F4E9", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#47F29C88" } },
