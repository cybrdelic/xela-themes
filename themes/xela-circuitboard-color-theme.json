    { "scope": ["variable.parameter","meta.parameter","meta.function.parameters"], "settings": { "foreground": "#E0F4E9" } },
    { "scope": ["variable.other.property","support.variable.property","meta.object-literal.key","meta.mapping.key","string.unquoted.label.js"], "settings": { "foreground": "#EEFFF6" } },
    { "scope": ["keyword.control.import","keyword.control.export","support.module","entity.name.namespace"], "settings": { "foreground": "#6DE6B2" } },
