  "tokenColors": [
    { "scope": ["source","meta.brace","meta.delimiter","punctuation","punctuation.definition.parameters","punctuation.definition.block"], "settings": { "foreground": "#EEFFF6" } },
    { "scope": ["comment","punctuation.definition.comment"], "settings": { "foreground": "#8FA79A" } },
    { "scope": ["comment.block.documentation","comment.line.documentation"], "settings": { "foreground": "#98B8A7" } },
