    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#E0F4E9" } },
    { "scope": ["variable.language.self","variable.language.this"], "settings": { "foreground": "#A1C1B1" } },
    { "scope": ["invalid.deprecated","invalid.illegal"], "settings": { "foreground": "#FFE9EF" } },

    { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
    { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#E0F4E9", "fontStyle": "bold" } },
    { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#47F29C", "fontStyle": "bold" } },
    { "scope": ["heading.3.markdown"], "settings": { "foreground": "#2BE084", "fontStyle": "bold" } },
    { "scope": ["heading.4.markdown"], "settings": { "foreground": "#6DE6B2", "fontStyle": "bold" } },
