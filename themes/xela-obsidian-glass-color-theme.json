{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Obsidian Glass â€” Black + Glass",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#7DD3FCAA",
    "selection.background": "#7DD3FC33",

    "titleBar.activeBackground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.activeForeground": "#EAF2FF",
    "titleBar.inactiveForeground": "#A6AAB4",
    "titleBar.border": "#0F0F0F",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#EAF2FF",
    "activityBar.inactiveForeground": "#9AA0AB",
    "activityBar.activeBorder": "#7DD3FC",
    "activityBar.border": "#0F0F0F",
    "activityBarBadge.background": "#7DD3FC",
    "activityBarBadge.foreground": "#03121E",

    "sideBar.background": "#000000",
    "sideBar.foreground": "#EAF2FF",
    "sideBar.border": "#0F0F0F",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#EAF2FF",
    "statusBar.border": "#0F0F0F",

    "panel.background": "#000000",
    "panel.border": "#0F0F0F",

    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.tabsBorder": "#0D0D0D",

    "tab.activeBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "tab.border": "#000000",
    "tab.activeForeground": "#EAF2FF",
    "tab.inactiveForeground": "#A6AAB4",
    "tab.activeBorderTop": "#7DD3FC",

    "editor.background": "#000000",
    "editor.foreground": "#EAF2FF",
    "editor.lineHighlightBackground": "#0D0D0D",
    "editor.selectionBackground": "#7DD3FC33",
    "editorCursor.foreground": "#7DD3FC",

    "editorHoverWidget.background": "#0A0A0ACC",
    "editorHoverWidget.border": "#1E293B",
    "editorHoverWidget.foreground": "#EAF2FF",

    "editorWidget.background": "#0A0A0ACC",
    "editorWidget.border": "#1E293B",
    "widget.shadow": "#000000CC",
    "dropdown.background": "#0A0A0A",
    "dropdown.border": "#1E293B",
    "quickInput.background": "#0A0A0ACC",
    "quickInput.foreground": "#EAF2FF",

    "list.activeSelectionBackground": "#0F0F0F",
    "list.hoverBackground": "#0D0D0D",
    "list.focusBackground": "#0A0A0A",

    "button.background": "#7DD3FC",
    "button.hoverBackground": "#BAE6FD",
    "button.foreground": "#03121E",

    "terminal.background": "#000000",
    "terminal.foreground": "#EAF2FF",
    "terminalCursor.foreground": "#7DD3FC",
    "terminal.ansiBlue": "#7DD3FC",
    "terminal.ansiCyan": "#67E8F9",
    "terminal.ansiMagenta": "#F0ABFC",
    "terminal.ansiGreen": "#5EEAD4",
    "terminal.ansiYellow": "#FDE68A",
    "terminal.ansiRed": "#FB7185",
    "terminal.ansiBrightBlue": "#93C5FD",
    "terminal.ansiBrightCyan": "#A5F3FC",
    "terminal.ansiBrightMagenta": "#E9D5FF",
    "terminal.ansiBrightGreen": "#99F6E4",
    "terminal.ansiBrightYellow": "#FEF3C7",
    "terminal.ansiBrightRed": "#FCA5A5",
    "terminal.ansiWhite": "#F5F7FA",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#475569",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["keyword"], "settings": { "foreground": "#93C5FD", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#67E8F9" } },
    { "scope": ["string"], "settings": { "foreground": "#99F6E4" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#FDE68A" } },
  { "scope": ["comment"], "settings": { "foreground": "#94A3B8", "fontStyle": "italic" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#EAF2FF", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#7DD3FC", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#67E8F9", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#F0ABFC", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#99F6E4", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#94A3B8", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#F5F7FA", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#93C5FD" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#EAF2FF", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#94A3B8" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "fontStyle": "strikethrough", "foreground": "#FCA5A5" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#7DD3FC", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#EAF2FF" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#F0ABFC" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#94A3B8" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#FDE68A" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#FDE68A" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#1E293B" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#94A3B8", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#7DD3FC" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#1E293B" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FDE68A" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FDE68A" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#EAF2FF" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#93C5FD", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#1E293B" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#94A3B8" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#5EEAD4" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#FB7185" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#FDE68A" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#7DD3FC" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#7DD3FC" } }
  ],
  "semanticTokenColors": {
    "function": "#67E8F9",
    "string": "#99F6E4",
    "number": "#FDE68A",
    "keyword": "#93C5FD",
    "type": "#E9D5FF"
  }
}
