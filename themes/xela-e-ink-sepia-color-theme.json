{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA E‑Ink Sepia — Paper Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#A16207AA",
    "selection.background": "#F59E0B22",

    "titleBar.activeBackground": "#FAF7F2",
    "titleBar.inactiveBackground": "#FAF7F2",
    "titleBar.activeForeground": "#4B4539",
    "titleBar.inactiveForeground": "#6B5E49",
    "titleBar.border": "#E7E0D6",

    "activityBar.background": "#FAF7F2",
    "activityBar.foreground": "#3F3A31",
    "activityBar.activeBorder": "#D97706",
    "activityBar.border": "#E7E0D6",

    "sideBar.background": "#F4EFE7",
    "sideBar.foreground": "#3F3A31",
    "sideBar.border": "#E7E0D6",

    "statusBar.background": "#FAF7F2",
    "statusBar.foreground": "#3F3A31",
    "statusBar.border": "#E7E0D6",

    "panel.background": "#FAF7F2",
    "panel.border": "#E7E0D6",

    "editorGroupHeader.tabsBackground": "#FAF7F2",
    "editorGroupHeader.tabsBorder": "#E7E0D6",

    "tab.activeBackground": "#FAF7F2",
    "tab.inactiveBackground": "#FAF7F2",
    "tab.border": "#FAF7F2",
    "tab.activeForeground": "#3F3A31",
    "tab.inactiveForeground": "#6B5E49",
    "tab.activeBorderTop": "#D97706",

    "editor.background": "#FFFCF7",
    "editor.foreground": "#3F3A31",
    "editor.lineHighlightBackground": "#F4EFE7",
    "editor.selectionBackground": "#F59E0B22",
    "editorCursor.foreground": "#A16207",

    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#E7E0D6",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#E7E0D6",

    "list.activeSelectionBackground": "#EFE8DD",
    "list.hoverBackground": "#F4EFE7",

    "button.background": "#D97706",
    "button.hoverBackground": "#F59E0B",
    "button.foreground": "#2B2318",

    "terminal.background": "#FFFCF7",
    "terminal.foreground": "#3F3A31",
    "terminalCursor.foreground": "#D97706",
    "terminal.ansiBlack": "#2B2318",
    "terminal.ansiRed": "#8B5E34",
    "terminal.ansiGreen": "#6B8E23",
    "terminal.ansiYellow": "#D97706",
    "terminal.ansiBlue": "#6B7AA1",
    "terminal.ansiMagenta": "#8B6FA5",
    "terminal.ansiCyan": "#5F9EA0",
    "terminal.ansiWhite": "#F0EDE6",
    "terminal.ansiBrightBlack": "#5C5346",
    "terminal.ansiBrightRed": "#A47148",
    "terminal.ansiBrightGreen": "#7BAA2A",
    "terminal.ansiBrightYellow": "#F59E0B",
    "terminal.ansiBrightBlue": "#7C8BB2",
    "terminal.ansiBrightMagenta": "#A88BC5",
    "terminal.ansiBrightCyan": "#7BB6B7",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["keyword"], "settings": { "foreground": "#A16207", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#6B7AA1" } },
    { "scope": ["string"], "settings": { "foreground": "#7BAA2A" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#D97706" } },
    { "scope": ["comment"], "settings": { "foreground": "#7D7465", "fontStyle": "italic" } },
    { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
    { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#3F3A31", "fontStyle": "bold" } },
    { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#D97706", "fontStyle": "bold" } },
    { "scope": ["heading.3.markdown"], "settings": { "foreground": "#6B7AA1", "fontStyle": "bold" } },
    { "scope": ["heading.4.markdown"], "settings": { "foreground": "#8B6FA5", "fontStyle": "bold" } },
    { "scope": ["heading.5.markdown"], "settings": { "foreground": "#7BAA2A", "fontStyle": "bold" } },
    { "scope": ["heading.6.markdown"], "settings": { "foreground": "#7D7465", "fontStyle": "bold" } },
    { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#3F3A31", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#D97706" } },
    { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#3F3A31", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#3F3A3188" } },
    { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "fontStyle": "strikethrough", "foreground": "#8B5E34" } },
    { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#6B7AA1", "fontStyle": "underline" } },
    { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#3F3A31" } },
    { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#8B6FA5" } },
    { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#7D7465" } },
    { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#3F3A31" } },
    { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#3F3A31" } },
    { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#E7E0D6" } },
    { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#6B5E49", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#D97706" } },
    { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#E7E0D6" } },
    { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#3F3A31" } },
    { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#D97706" } },
    { "scope": ["markup.table"], "settings": { "foreground": "#3F3A31" } },
    { "scope": ["markup.table.header"], "settings": { "foreground": "#6B7AA1", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#E7E0D6" } },
    { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#7D7465" } },
    { "scope": ["markup.inserted"], "settings": { "foreground": "#7BAA2A" } },
    { "scope": ["markup.deleted"], "settings": { "foreground": "#8B5E34" } },
    { "scope": ["markup.changed"], "settings": { "foreground": "#D97706" } },
    { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#D97706" } },
    { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#6B7AA1" } }
  ],
  "semanticTokenColors": {
    "function": "#6B7AA1",
    "string": "#7BAA2A",
    "number": "#D97706",
    "keyword": "#A16207",
    "type": "#8B6FA5"
  }
}
