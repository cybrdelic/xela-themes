    "keyword": { "foreground": "#00FF87", "bold": true },
    "modifier": "#35FBA9",
    "comment": { "foreground": "#4C8A70", "italic": true },
    "string": "#5EF2B0",
    "number": "#DFFF9E",
    "regexp": "#5EF2B0",
    "boolean": "#C3FF7A",
    "operator": "#35FBA9",
    "parameter.label": "#62FFB5",
