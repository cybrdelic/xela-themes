    { "scope": ["markup.inserted", "meta.diff.range", "markup.inserted.git_gutter"], "settings": { "foreground": "#35FBA9" } },
    { "scope": ["markup.deleted", "markup.deleted.git_gutter"], "settings": { "foreground": "#FF4D5E" } },
    { "scope": ["markup.changed", "markup.changed.git_gutter"], "settings": { "foreground": "#FFD866" } },
    { "scope": ["invalid", "invalid.illegal"], "settings": { "foreground": "#FF4D5E", "fontStyle": "underline" } },
    { "scope": ["invalid.deprecated"], "settings": { "foreground": "#FFD866", "fontStyle": "underline" } },
    { "scope": ["meta.embedded", "meta.embedded.block"], "settings": { "foreground": "#B9FDE4" } },
    { "scope": ["constant.other.object.key", "meta.object-literal.key", "support.type.property-name.json"], "settings": { "foreground": "#62FFB5" } },
    { "scope": ["string.json"], "settings": { "foreground": "#5EF2B0" } },
    { "scope": ["punctuation.separator.key-value"], "settings": { "foreground": "#2F6C53" } },
    { "scope": ["entity.name.section"], "settings": { "foreground": "#7AFFC2", "fontStyle": "bold" } },
    { "scope": ["meta.annotation", "punctuation.decorator"], "settings": { "foreground": "#35FBA9", "fontStyle": "italic" } },
    { "scope": ["meta.attribute.scala", "entity.other.attribute-name.pseudo-class.css"], "settings": { "foreground": "#62FFB5", "fontStyle": "italic" } },
    { "scope": ["source.rust support.macro"], "settings": { "foreground": "#72FFD4", "fontStyle": "bold" } },
    { "scope": ["source.rust entity.name.lifetime"], "settings": { "foreground": "#53E3B0" } },
    { "scope": ["source.python support.type", "source.python support.variable.magic"], "settings": { "foreground": "#62FFB5", "fontStyle": "italic" } },
    { "scope": ["source.python constant.character.format.placeholder.other"], "settings": { "foreground": "#DFFF9E" } },
    { "scope": ["source.shell variable.other", "source.shell support.variable"], "settings": { "foreground": "#62FFB5" } },
    { "scope": ["source.shell punctuation.definition.variable"], "settings": { "foreground": "#35FBA9" } }
