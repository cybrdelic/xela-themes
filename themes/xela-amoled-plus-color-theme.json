{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA AMOLED+ â€” Pure Black Max",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#00FFA0AA",
    "selection.background": "#72FACA33",
    "window.activeBorder": "#00FFA0",
    "window.inactiveBorder": "#141414",

    "titleBar.activeBackground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.activeForeground": "#F2F4F8",
    "titleBar.inactiveForeground": "#A6AAB4",
    "titleBar.border": "#0F0F0F",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#EDF1F5",
    "activityBar.inactiveForeground": "#9AA0AB",
    "activityBar.activeBorder": "#00FFA0",
    "activityBar.border": "#0F0F0F",
    "activityBarBadge.background": "#00FFA0",
    "activityBarBadge.foreground": "#04110C",

    "sideBar.background": "#000000",
    "sideBar.foreground": "#EDEFF2",
    "sideBar.border": "#0F0F0F",
    "sideBarTitle.foreground": "#B7BAC4",
    "sideBarSectionHeader.background": "#000000",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#E0E3EA",
    "statusBar.border": "#0F0F0F",
    "statusBarItem.hoverBackground": "#111111",
    "statusBarItem.prominentBackground": "#0D0D0D",
    "statusBarItem.prominentHoverBackground": "#141414",
    "statusBar.debuggingBackground": "#72FACA",
    "statusBar.debuggingForeground": "#06110E",

    "panel.background": "#000000",
    "panel.border": "#0F0F0F",
    "panelTitle.activeForeground": "#F7F8FA",
    "panelTitle.inactiveForeground": "#B7BAC4",
    "panelTitle.activeBorder": "#F5F2E8",

    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.tabsBorder": "#0D0D0D",
    "editorGroup.dropBackground": "#00FFA022",

    "tab.activeBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "tab.border": "#000000",
    "tab.activeForeground": "#F7F8FA",
    "tab.inactiveForeground": "#A6AAB4",
    "tab.activeBorderTop": "#F5F2E8",
    "tab.unfocusedActiveBorderTop": "#72FACA88",
    "tab.hoverBackground": "#0A0A0A",
    "tab.hoverForeground": "#FFFFFF",
    "tab.unfocusedHoverBackground": "#0A0A0A",
    "tab.activeModifiedBorder": "#72FACA",
    "tab.hoverBorder": "#141414",

    "editor.background": "#000000",
    "editor.foreground": "#F7F8FA",
    "editor.lineHighlightBackground": "#0D0D0D",
    "editor.selectionBackground": "#72FACA33",
    "editor.inactiveSelectionBackground": "#72FACA22",
    "editor.selectionHighlightBackground": "#72FACA22",
    "editor.selectionHighlightBorder": "#72FACA55",
    "editor.wordHighlightBackground": "#00FFA01A",
    "editor.wordHighlightStrongBackground": "#00FFA02B",
    "editor.foldBackground": "#0A0A0A80",
    "editorCursor.foreground": "#00FFA0",
    "editorHoverWidget.background": "#0A0A0A",
    "editorHoverWidget.border": "#242424",
    "editorHoverWidget.foreground": "#F7F8FA",
    "editorHoverWidget.highlightForeground": "#00FFA0",
    "editorHoverWidget.statusBarBackground": "#0A0A0A",

    "editorIndentGuide.background1": "#2A2A2A",
    "editorIndentGuide.activeBackground1": "#4D4D4D",
    "editorLineNumber.foreground": "#4E5057",
    "editorLineNumber.activeForeground": "#EDEFF2",

    "editorBracketMatch.background": "#141414",
    "editorBracketMatch.border": "#72FACA88",
    "editorBracketHighlight.foreground1": "#72FACA",
    "editorBracketHighlight.foreground2": "#FFD166",
    "editorBracketHighlight.foreground3": "#FF8BDA",
    "editorBracketHighlight.foreground4": "#D8C8FF",
    "editorBracketHighlight.foreground5": "#79E49A",
    "editorBracketHighlight.foreground6": "#F7B883",
    "editorBracketPairGuide.activeBackground1": "#72FACA55",

    "editorInlayHint.foreground": "#C9D1D9",
    "editorInlayHint.background": "#111111",
    "editorGhostText.foreground": "#7D828E",

    "list.activeSelectionBackground": "#0F0F0F",
    "list.activeSelectionForeground": "#F7F8FA",
    "list.hoverBackground": "#0D0D0D",
    "list.focusBackground": "#0A0A0A",

    "quickInput.background": "#0A0A0ACC",
    "quickInput.foreground": "#F7F8FA",

    "input.background": "#0A0A0A",
    "input.foreground": "#F7F8FA",
    "input.border": "#242424",
    "input.placeholderForeground": "#7D828E",

    "dropdown.background": "#0A0A0A",
    "dropdown.border": "#242424",
    "dropdown.foreground": "#F7F8FA",

    "button.background": "#00FFA0",
    "button.hoverBackground": "#72FACA",
    "button.foreground": "#04110C",

    "terminal.background": "#000000",
    "terminal.foreground": "#F7F8FA",
    "terminalCursor.foreground": "#00FFA0",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF5A87",
    "terminal.ansiGreen": "#22C55E",
    "terminal.ansiYellow": "#FFD166",
    "terminal.ansiBlue": "#D8C8FF",
    "terminal.ansiMagenta": "#FF3CAC",
    "terminal.ansiCyan": "#00FFA0",
    "terminal.ansiWhite": "#E5E7EB",
    "terminal.ansiBrightBlack": "#4B5563",
    "terminal.ansiBrightGreen": "#00FFA0",
    "terminal.ansiBrightCyan": "#72FACA",
    "terminal.ansiBrightYellow": "#FFD166",
    "terminal.ansiBrightMagenta": "#FF66C4",
    "terminal.ansiBrightWhite": "#F9FAFB"
  },
  "tokenColors": [
    { "scope": ["keyword","keyword.control","storage.type","storage.modifier"], "settings": { "foreground": "#FF3CAC", "fontStyle": "bold" } },
    { "scope": ["entity.name.function","variable.function"], "settings": { "foreground": "#FF7A00" } },
    { "scope": ["string","string.quoted","string.template"], "settings": { "foreground": "#00FFA0" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#F2C97D" } },
  { "scope": ["comment"], "settings": { "foreground": "#8E93A6", "fontStyle": "italic" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#F7F8FA", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#00FFA0", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#FF3CAC", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#D8C8FF", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#79E49A", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#8E93A6", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#FF3CAC" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#EDEFF2", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#A6AAB4" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "fontStyle": "strikethrough", "foreground": "#FF5A87" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#00FFA0", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#F7F8FA" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#D8C8FF" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#8E93A6" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#F2C97D" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#F2C97D" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#242424" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#9AA0AB", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#00FFA0" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#242424" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FFD166" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FFD166" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#F7F8FA" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#D8C8FF", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#242424" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#8E93A6" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#22C55E" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#FF5A87" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#FFD166" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#00FFA0" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#7DD3FC" } }
  ],
  "semanticTokenColors": {
    "function": "#FF7A00",
    "method": "#FF7A00",
    "string": "#00FFA0",
    "number": "#F2C97D",
    "boolean": "#79E49A",
    "keyword": "#FF3CAC",
    "type": "#C8B0FF"
  }
}
