{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Vapor â€” Neon Retrowave",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "titleBar.activeBackground": "#1A0033",
    "titleBar.inactiveBackground": "#1A0033",
    "titleBar.activeForeground": "#EAD7FF",
    "titleBar.inactiveForeground": "#BDA6DB",
    "titleBar.border": "#2B0055",
    "activityBar.background": "#0D001A",
    "activityBar.foreground": "#F7E9FF",
    "activityBar.inactiveForeground": "#BDA6DB",
    "activityBar.activeBorder": "#FF00FF",
    "activityBarBadge.background": "#00FFFF",
    "activityBarBadge.foreground": "#001A1A",
    "sideBar.background": "#110026",
    "sideBar.foreground": "#EAD7FF",
    "sideBar.border": "#2B0055",
    "statusBar.background": "#0D001A",
    "statusBar.foreground": "#EAD7FF",
    "statusBar.border": "#2B0055",
    "panel.background": "#110026",
    "panel.border": "#2B0055",
    "editorGroupHeader.tabsBackground": "#110026",
    "tab.activeBackground": "#1A0033",
    "tab.inactiveBackground": "#110026",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveForeground": "#C9B1E6",
    "tab.activeBorderTop": "#FF00FF",
    "editor.background": "#1A0033",
    "editor.foreground": "#EAD7FF",
    "editor.lineHighlightBackground": "#2B0055",
    "editor.selectionBackground": "#FF00FF33",
    "editor.inactiveSelectionBackground": "#00FFFF22",
    "editor.findMatchBackground": "#00000000",
    "editor.findMatchBorder": "#FF00FF",
    "editor.findMatchHighlightBackground": "#33003380",
    "editor.hoverHighlightBackground": "#001A1A80",
    "editorCursor.foreground": "#00FFFF",
    "editorIndentGuide.background1": "#2E0A52",
    "editorIndentGuide.activeBackground1": "#4C1485",
    "editorBracketHighlight.foreground1": "#FF00FF",
    "editorBracketHighlight.foreground2": "#00FFFF",
    "editorBracketHighlight.foreground3": "#FFB86C",
    "editorBracketPairGuide.activeBackground1": "#FF00FF55",
    "editorBracketPairGuide.activeBackground2": "#00FFFF55",
    "editorInlayHint.foreground": "#EAD7FF",
    "editorInlayHint.background": "#2B0055",
    "editorInlayHint.parameterForeground": "#FFD1FF",
    "editorInlayHint.typeForeground": "#B3F9FF",
    "list.activeSelectionBackground": "#24003F",
    "list.hoverBackground": "#1F0036",
    "quickInput.background": "#16002B",
    "input.background": "#16002B",
    "input.border": "#3A0073",
    "button.background": "#FF00FF",
    "button.hoverBackground": "#FF66FF",
    "badge.background": "#00FFFF",
    "badge.foreground": "#001A1A",
    "textLink.foreground": "#00FFFF",
    "textLink.activeForeground": "#66FFFF",
    "textPreformat.foreground": "#FFD1FF",
    "textBlockQuote.background": "#16002B",
    "textBlockQuote.border": "#3A0073",
    "textCodeBlock.background": "#120024",
    "textSeparator.foreground": "#3A0073",
    "terminal.background": "#1A0033",
    "terminal.foreground": "#EAD7FF",
    "terminalCursor.foreground": "#00FFFF",
    "terminal.ansiBlack": "#0D001A",
    "terminal.ansiRed": "#FF3366",
    "terminal.ansiGreen": "#00E5FF",
    "terminal.ansiYellow": "#FFB86C",
    "terminal.ansiBlue": "#9E6EFF",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#F7F0FF",
    "terminal.ansiBrightBlack": "#4B2E66",
    "terminal.ansiBrightRed": "#FF5A87",
    "terminal.ansiBrightGreen": "#4CFFCA",
    "terminal.ansiBrightYellow": "#FFD166",
    "terminal.ansiBrightBlue": "#C1A6FF",
    "terminal.ansiBrightMagenta": "#FF66FF",
    "terminal.ansiBrightCyan": "#66FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["source","meta.brace","punctuation"], "settings": { "foreground": "#EAD7FF" } },
    { "scope": ["comment"], "settings": { "foreground": "#BDA6DB" } },
    { "scope": ["keyword","storage.type","storage.modifier"], "settings": { "foreground": "#FF66FF", "fontStyle": "bold" } },
    { "scope": ["entity.name.function","support.function"], "settings": { "foreground": "#66FFFF" } },
    { "scope": ["string"], "settings": { "foreground": "#FF66B2" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#FFFF66" } },
    { "scope": ["entity.name.type","support.type"], "settings": { "foreground": "#C1A6FF" } },
    { "scope": ["string.regexp"], "settings": { "foreground": "#4CFFCA" } },
  { "scope": ["invalid","invalid.illegal"], "settings": { "foreground": "#FFF0F5" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#66FFFF", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#FF66FF", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#FFB86C", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#C1A6FF", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#4CFFCA", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#BDA6DB", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#FF66FF" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#C1A6FF", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#BDA6DB" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "foreground": "#FF3366", "fontStyle": "strikethrough" } },
  { "scope": ["markup.underline","markup.underline.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#00FFFF", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#EAD7FF" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#C1A6FF" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#BDA6DB" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#FFD1FF" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#FFD1FF" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#3A0073" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#BDA6DB", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#FF66FF" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#3A0073" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FFD166" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FFD166" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#C1A6FF" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#66FFFF", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#3A0073" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#9E8AB8" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#4CFFCA" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#FF3366" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#FFB86C" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#00FFFF" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#00FFFF" } }
  ],
  "semanticTokenColors": {
    "operator": "#FF66FF",
    "function": "#66FFFF",
    "type": "#C1A6FF",
    "string": "#FF66B2",
    "number": "#FFFF66",
    "variable": "#EAD7FF",
    "property": "#EAD7FF",
    "enumMember": "#FFD166"
  }
}
