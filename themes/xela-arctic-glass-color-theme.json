{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Arctic Glass â€” Crystal Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#06B6D4AA",
    "selection.background": "#06B6D422",

    "titleBar.activeBackground": "#F7FBFE",
    "titleBar.inactiveBackground": "#F7FBFE",
    "titleBar.activeForeground": "#0F172A",
    "titleBar.inactiveForeground": "#334155",
    "titleBar.border": "#E2E8F0",

    "activityBar.background": "#F7FBFE",
    "activityBar.foreground": "#0F172A",
    "activityBar.activeBorder": "#06B6D4",
    "activityBar.border": "#E2E8F0",

    "sideBar.background": "#F2F8FC",
    "sideBar.foreground": "#0F172A",
    "sideBar.border": "#E2E8F0",

    "statusBar.background": "#F7FBFE",
    "statusBar.foreground": "#0F172A",
    "statusBar.border": "#E2E8F0",

    "panel.background": "#F7FBFE",
    "panel.border": "#E2E8F0",

    "editorGroupHeader.tabsBackground": "#F7FBFE",
    "editorGroupHeader.tabsBorder": "#E2E8F0",

    "tab.activeBackground": "#F7FBFE",
    "tab.inactiveBackground": "#F7FBFE",
    "tab.border": "#F7FBFE",
    "tab.activeForeground": "#0F172A",
    "tab.inactiveForeground": "#334155",
    "tab.activeBorderTop": "#06B6D4",

    "editor.background": "#FFFFFF",
    "editor.foreground": "#0F172A",
    "editor.lineHighlightBackground": "#ECF7FD",
    "editor.selectionBackground": "#06B6D422",
    "editorCursor.foreground": "#06B6D4",

    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#E2E8F0",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#E2E8F0",
    "widget.shadow": "#A3D6F080",

    "list.activeSelectionBackground": "#E6F6FD",
    "list.hoverBackground": "#ECF7FD",

    "button.background": "#06B6D4",
    "button.hoverBackground": "#22D3EE",
    "button.foreground": "#06232A",

    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#0F172A",
    "terminalCursor.foreground": "#06B6D4",
    "terminal.ansiBlack": "#0F172A",
    "terminal.ansiRed": "#EF4444",
    "terminal.ansiGreen": "#10B981",
    "terminal.ansiYellow": "#EAB308",
    "terminal.ansiBlue": "#3B82F6",
    "terminal.ansiMagenta": "#A855F7",
    "terminal.ansiCyan": "#06B6D4",
    "terminal.ansiWhite": "#F1F5F9",
    "terminal.ansiBrightBlack": "#334155",
    "terminal.ansiBrightRed": "#F87171",
    "terminal.ansiBrightGreen": "#34D399",
    "terminal.ansiBrightYellow": "#FACC15",
    "terminal.ansiBrightBlue": "#60A5FA",
    "terminal.ansiBrightMagenta": "#C084FC",
    "terminal.ansiBrightCyan": "#22D3EE",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["keyword"], "settings": { "foreground": "#0EA5E9", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#06B6D4" } },
    { "scope": ["string"], "settings": { "foreground": "#10B981" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#EAB308" } },
  { "scope": ["comment"], "settings": { "foreground": "#64748B", "fontStyle": "italic" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#0F172A", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#06B6D4", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#0EA5E9", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#60A5FA", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#10B981", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#94A3B8", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#0F172A", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#0EA5E9" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#0F172A", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#94A3B8" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "fontStyle": "strikethrough", "foreground": "#64748B" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#06B6D4", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#60A5FA" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#94A3B8" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#E2E8F0" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#334155", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#06B6D4" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#E2E8F0" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#06B6D4" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#0F172A" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#0F172A", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#E2E8F0" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#94A3B8" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#10B981" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#EF4444" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#EAB308" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#06B6D4" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#06B6D4" } }
  ],
  "semanticTokenColors": {
    "function": "#06B6D4",
    "string": "#10B981",
    "number": "#EAB308",
    "keyword": "#0EA5E9",
    "type": "#A855F7"
  }
}
