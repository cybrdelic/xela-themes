{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Terminalist â€” ANSI Native",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#0B0D10",
    "editor.foreground": "#E5E7EB",
    "editorCursor.foreground": "#E5E7EB",
    "editor.lineHighlightBackground": "#0F1216",
    "editor.selectionBackground": "#93C5FD22",

    "terminal.background": "#0B0D10",
    "terminal.foreground": "#E5E7EB",
    "terminalCursor.foreground": "#FFFFFF",

    "terminal.ansiBlack": "#111827",
    "terminal.ansiRed": "#EF4444",
    "terminal.ansiGreen": "#22C55E",
    "terminal.ansiYellow": "#F59E0B",
    "terminal.ansiBlue": "#3B82F6",
    "terminal.ansiMagenta": "#A855F7",
    "terminal.ansiCyan": "#06B6D4",
    "terminal.ansiWhite": "#D1D5DB",
    "terminal.ansiBrightBlack": "#374151",
    "terminal.ansiBrightRed": "#F87171",
    "terminal.ansiBrightGreen": "#4ADE80",
    "terminal.ansiBrightYellow": "#FBBF24",
    "terminal.ansiBrightBlue": "#60A5FA",
    "terminal.ansiBrightMagenta": "#C084FC",
    "terminal.ansiBrightCyan": "#22D3EE",
    "terminal.ansiBrightWhite": "#FFFFFF",

    "statusBar.background": "#0B0D10",
    "statusBar.foreground": "#E5E7EB",
    "statusBar.border": "#1F242B",

    "titleBar.activeBackground": "#0B0D10",
    "titleBar.inactiveBackground": "#0B0D10",
    "titleBar.activeForeground": "#E5E7EB",
    "titleBar.inactiveForeground": "#9CA3AF",
    "titleBar.border": "#1F242B",

    "activityBar.background": "#0B0D10",
    "activityBar.foreground": "#E5E7EB",
    "activityBar.border": "#1F242B",

    "sideBar.background": "#0B0D10",
    "sideBar.foreground": "#E5E7EB",
    "sideBar.border": "#1F242B",

    "panel.background": "#0B0D10",
    "panel.border": "#1F242B",

    "editorGroupHeader.tabsBackground": "#0B0D10",
    "editorGroupHeader.tabsBorder": "#1F242B",

    "tab.activeBackground": "#0B0D10",
    "tab.inactiveBackground": "#0B0D10",
    "tab.border": "#1F242B",
    "tab.activeForeground": "#E5E7EB",
    "tab.inactiveForeground": "#9CA3AF",

    "list.activeSelectionBackground": "#111827",
    "list.hoverBackground": "#0F1216",
    "list.focusBackground": "#111827",

    "editorBracketHighlight.foreground1": "#EF4444",
    "editorBracketHighlight.foreground2": "#22C55E",
    "editorBracketHighlight.foreground3": "#F59E0B",
    "editorBracketHighlight.foreground4": "#3B82F6",
    "editorBracketHighlight.foreground5": "#A855F7",
    "editorBracketHighlight.foreground6": "#06B6D4",

    "textLink.foreground": "#60A5FA",
    "editorLink.activeForeground": "#60A5FA",

    "editorWhitespace.foreground": "#FFFFFF13",
    "editorIndentGuide.background1": "#FFFFFF12",
    "editorIndentGuide.activeBackground1": "#FFFFFF28",

    "editorWidget.background": "#0D1014F2",
    "editorWidget.border": "#1F242B",
    "editorHoverWidget.background": "#0D1014F2",
    "editorHoverWidget.border": "#1F242B"
  },
  "tokenColors": [
    { "scope": ["comment"], "settings": { "foreground": "#6B7280", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage"], "settings": { "foreground": "#EF4444", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#3B82F6" } },
    { "scope": ["entity.name.type"], "settings": { "foreground": "#A855F7" } },
    { "scope": ["variable"], "settings": { "foreground": "#D1D5DB" } },
    { "scope": ["string"], "settings": { "foreground": "#22C55E" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#F59E0B" } },
    { "scope": ["constant.language"], "settings": { "foreground": "#06B6D4" } },
  { "scope": ["invalid"], "settings": { "foreground": "#F87171" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#60A5FA", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#22C55E", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#F59E0B", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#A855F7", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#06B6D4", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#E5E7EB", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#EF4444" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#E5E7EB", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#9CA3AF" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "fontStyle": "strikethrough", "foreground": "#F87171" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#60A5FA", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#E5E7EB" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#D1D5DB" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#9CA3AF" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#F59E0B" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#F59E0B" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#374151" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#9CA3AF", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#22C55E" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#1F242B" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#06B6D4" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#06B6D4" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#E5E7EB" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#60A5FA", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#1F242B" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#6B7280" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#22C55E" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#EF4444" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#F59E0B" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#22C55E" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#60A5FA" } }
  ],
  "semanticTokenColors": {
    "comment": "#6B7280",
    "keyword": "#EF4444",
    "function": "#3B82F6",
    "type": "#A855F7",
    "variable": "#D1D5DB",
    "string": "#22C55E",
    "number": "#F59E0B",
    "enumMember": "#06B6D4"
  }
}
