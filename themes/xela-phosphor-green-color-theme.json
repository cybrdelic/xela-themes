{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Phosphor Green â€” Monochrome CRT",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#00FF7FAA",
    "selection.background": "#00FF7F22",

    "titleBar.activeBackground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.activeForeground": "#CCFFDD",
    "titleBar.inactiveForeground": "#88AA99",
    "titleBar.border": "#0F0F0F",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#CCFFDD",
    "activityBar.activeBorder": "#00FF7F",
    "activityBar.border": "#0F0F0F",

    "sideBar.background": "#000000",
    "sideBar.foreground": "#B8FFCE",
    "sideBar.border": "#0F0F0F",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#B8FFCE",
    "statusBar.border": "#0F0F0F",

    "panel.background": "#000000",
    "panel.border": "#0F0F0F",

    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.tabsBorder": "#0D0D0D",

    "tab.activeBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "tab.border": "#000000",
    "tab.activeForeground": "#CCFFDD",
    "tab.inactiveForeground": "#88AA99",
    "tab.activeBorderTop": "#00FF7F",

    "editor.background": "#000000",
    "editor.foreground": "#CCFFDD",
    "editor.lineHighlightBackground": "#0D0D0D",
    "editor.selectionBackground": "#00FF7F22",
    "editorCursor.foreground": "#00FF7F",

    "editorHoverWidget.background": "#00110A",
    "editorHoverWidget.border": "#0A3322",
    "editorWidget.background": "#00110A",
    "editorWidget.border": "#0A3322",

    "list.activeSelectionBackground": "#0A0A0A",
    "list.hoverBackground": "#0D0D0D",

    "button.background": "#00FF7F",
    "button.hoverBackground": "#33FF99",
    "button.foreground": "#00110A",

    "terminal.background": "#000000",
    "terminal.foreground": "#CCFFDD",
    "terminalCursor.foreground": "#00FF7F",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#00AA55",
    "terminal.ansiGreen": "#00FF7F",
    "terminal.ansiYellow": "#55FF99",
    "terminal.ansiBlue": "#88FFBB",
    "terminal.ansiMagenta": "#66FFAA",
    "terminal.ansiCyan": "#AAFFDD",
    "terminal.ansiWhite": "#EEFFFA",
    "terminal.ansiBrightBlack": "#335544",
    "terminal.ansiBrightRed": "#22CC77",
    "terminal.ansiBrightGreen": "#00FF7F",
    "terminal.ansiBrightYellow": "#88FFBB",
    "terminal.ansiBrightBlue": "#BBFFDD",
    "terminal.ansiBrightMagenta": "#99FFCC",
    "terminal.ansiBrightCyan": "#D6FFF0",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["keyword","storage.type"], "settings": { "foreground": "#00FF7F", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#88FFBB" } },
    { "scope": ["string"], "settings": { "foreground": "#55FF99" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#AAFFDD" } },
  { "scope": ["comment"], "settings": { "foreground": "#88AA99", "fontStyle": "italic" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#BBFFDD", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#00FF7F", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#55FF99", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#88FFBB", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#66FFAA", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#CCFFDD", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#EEFFFA", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#00FF7F" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#CCFFDD", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#88AA99" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "fontStyle": "strikethrough", "foreground": "#22CC77" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#88FFBB", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#EEFFFA" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#BBFFDD" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#88AA99" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#55FF99" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#55FF99" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#0A3322" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#88AA99", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#00FF7F" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#0F0F0F" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#66FFAA" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#66FFAA" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#CCFFDD" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#88FFBB", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#0A3322" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#88AA99" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#00FF7F" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#22CC77" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#66FFAA" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#00FF7F" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#88FFBB" } }
  ],
  "semanticTokenColors": {
    "function": "#88FFBB",
    "string": "#55FF99",
    "number": "#AAFFDD",
    "keyword": "#00FF7F",
    "type": "#AAFFDD"
  }
}
