  "tokenColors": [
    { "scope": ["source","meta.brace","meta.delimiter","punctuation","punctuation.definition.parameters","punctuation.definition.block"], "settings": { "foreground": "#F7F8FA" } },
    { "scope": ["comment","punctuation.definition.comment"], "settings": { "foreground": "#8E93A6", "fontStyle": "" } },
    { "scope": ["comment.block.documentation","comment.line.documentation"], "settings": { "foreground": "#9AA0B4", "fontStyle": "" } },
    { "scope": ["keyword.other.todo","keyword.other.fixme","keyword.other.xxx"], "settings": { "foreground": "#FFD166", "fontStyle": "bold" } },
    { "scope": ["keyword","keyword.control","storage.type","storage.modifier","keyword.operator.new","keyword.operator.expression"], "settings": { "foreground": "#FF3CAC", "fontStyle": "bold" } },
    { "scope": ["entity.name.function","variable.function","meta.function-call","support.function.any-method"], "settings": { "foreground": "#FF7A00" } },
    { "scope": ["string","string.quoted","string.template"], "settings": { "foreground": "#00F5A0" } },
    { "scope": ["constant.numeric","variable.other.constant","entity.name.constant","support.constant"], "settings": { "foreground": "#F2C97D" } },
    { "scope": ["constant.language.boolean","constant.boolean"], "settings": { "foreground": "#79E49A", "fontStyle": "bold" } },
    { "scope": ["constant.language.null","constant.language.undefined","constant.language.nil","constant.language.none"], "settings": { "foreground": "#A6ACBA", "fontStyle": "" } },
    { "scope": ["string.regexp","constant.other.character-class.regexp","constant.character.escape"], "settings": { "foreground": "#98FF66" } },
    { "scope": ["entity.name.type","support.type","meta.type","storage.type.class","storage.type.interface","entity.name.class","entity.other.inherited-class","entity.name.type.parameter"], "settings": { "foreground": "#C8B0FF" } },
    { "scope": ["support.function.builtin","support.variable.property.dom","support.variable.dom","support.type.property-name.json"], "settings": { "foreground": "#D74FA2" } },
    { "scope": ["meta.decorator","entity.name.function.decorator","storage.type.annotation","meta.annotation"], "settings": { "foreground": "#FF8BDA" } },
    { "scope": ["variable.parameter","meta.parameter","meta.function.parameters"], "settings": { "foreground": "#EADCB2", "fontStyle": "" } },
    { "scope": ["variable.other.property","support.variable.property","meta.object-literal.key","meta.mapping.key","string.unquoted.label.js"], "settings": { "foreground": "#F7F8FA" } },
    { "scope": ["keyword.control.import","keyword.control.export","support.module","entity.name.namespace"], "settings": { "foreground": "#F7B883" } },
    { "scope": ["support.class.component","entity.name.tag.tsx","entity.name.tag.jsx"], "settings": { "foreground": "#E0CCFF" } },
    { "scope": ["entity.name.tag.html","entity.name.tag"], "settings": { "foreground": "#F7B883" } },
    { "scope": ["punctuation.definition.tag","punctuation.section.embedded.begin.tsx","punctuation.section.embedded.end.tsx"], "settings": { "foreground": "#A6AAB4" } },
    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#EADCB2" } },
    { "scope": ["variable.language.self","variable.language.this"], "settings": { "foreground": "#A6ACBA", "fontStyle": "" } },
    { "scope": ["invalid.deprecated","invalid.illegal"], "settings": { "foreground": "#FFE9EF" } },
    { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
    { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#F2C97D", "fontStyle": "bold" } },
    { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#FF88C2", "fontStyle": "bold" } },
    { "scope": ["heading.3.markdown"], "settings": { "foreground": "#00F5A0", "fontStyle": "bold" } },
    { "scope": ["heading.4.markdown"], "settings": { "foreground": "#C8B0FF", "fontStyle": "bold" } },
    { "scope": ["heading.5.markdown"], "settings": { "foreground": "#F7B883", "fontStyle": "bold" } },
    { "scope": ["heading.6.markdown"], "settings": { "foreground": "#EADCB2", "fontStyle": "bold" } }
  ],
