    { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#FFCC0088" } },
    { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link"], "settings": { "foreground": "#007AFF", "fontStyle": "underline" } },
