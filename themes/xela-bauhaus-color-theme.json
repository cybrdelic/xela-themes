  "tokenColors": [
    { "scope": ["source","meta.brace","meta.delimiter","punctuation","punctuation.definition.parameters","punctuation.definition.block"], "settings": { "foreground": "#F7F8FA" } },
    { "scope": ["comment","punctuation.definition.comment"], "settings": { "foreground": "#8E93A6" } },
    { "scope": ["comment.block.documentation","comment.line.documentation"], "settings": { "foreground": "#9AA0B4" } },
    { "scope": ["keyword.other.todo","keyword.other.fixme","keyword.other.xxx"], "settings": { "foreground": "#FFCC00", "fontStyle": "bold" } },
    { "scope": ["keyword","keyword.control","storage.type","storage.modifier","keyword.operator.new","keyword.operator.expression"], "settings": { "foreground": "#007AFF", "fontStyle": "bold" } },
    { "scope": ["entity.name.function","variable.function","meta.function-call","support.function.any-method"], "settings": { "foreground": "#FF3B30" } },
    { "scope": ["string","string.quoted","string.template"], "settings": { "foreground": "#34C759" } },
    { "scope": ["constant.numeric","variable.other.constant","entity.name.constant","support.constant"], "settings": { "foreground": "#F2E7B9" } },
    { "scope": ["constant.language.boolean","constant.boolean"], "settings": { "foreground": "#34C759", "fontStyle": "bold" } },
    { "scope": ["constant.language.null","constant.language.undefined","constant.language.nil","constant.language.none"], "settings": { "foreground": "#A6ACBA" } },
