    { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#A6AAB4" } },
    { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#8E93A6" } },
    { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#A6AAB4", "fontStyle": "italic" } },
    { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#FFCC00" } },
    { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#202636" } },
    { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FFCC00" } },
    { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FFCC00" } },
    { "scope": ["markup.table"], "settings": { "foreground": "#F2F4F8" } },
    { "scope": ["markup.table.header"], "settings": { "foreground": "#AF52DE", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#202636" } },
    { "scope": ["markup.strikethrough"], "settings": { "foreground": "#A6ACBA", "fontStyle": "strikethrough" } },
    { "scope": ["markup.inserted"], "settings": { "foreground": "#34C759" } },
    { "scope": ["markup.deleted"], "settings": { "foreground": "#FF3B30" } },
    { "scope": ["markup.changed"], "settings": { "foreground": "#FFCC00" } },
    { "scope": ["markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#F2E7B9" } },
    { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#FFCC00" } },
    { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#007AFF" } }
