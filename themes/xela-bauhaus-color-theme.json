    { "scope": ["meta.decorator","entity.name.function.decorator","storage.type.annotation","meta.annotation"], "settings": { "foreground": "#AF52DE" } },
    { "scope": ["variable.parameter","meta.parameter","meta.function.parameters"], "settings": { "foreground": "#F2E7B9" } },
    { "scope": ["variable.other.property","support.variable.property","meta.object-literal.key","meta.mapping.key","string.unquoted.label.js"], "settings": { "foreground": "#F7F8FA" } },
    { "scope": ["keyword.control.import","keyword.control.export","support.module","entity.name.namespace"], "settings": { "foreground": "#007AFF" } },
    { "scope": ["support.class.component","entity.name.tag.tsx","entity.name.tag.jsx"], "settings": { "foreground": "#AF52DE" } },
    { "scope": ["entity.name.tag.html","entity.name.tag"], "settings": { "foreground": "#FFCC00" } },
    { "scope": ["punctuation.definition.tag","punctuation.section.embedded.begin.tsx","punctuation.section.embedded.end.tsx"], "settings": { "foreground": "#A6AAB4" } },
    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#F2E7B9" } },
    { "scope": ["variable.language.self","variable.language.this"], "settings": { "foreground": "#A6ACBA" } },
    { "scope": ["invalid.deprecated","invalid.illegal"], "settings": { "foreground": "#FFE9EF" } },

    { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
    { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#FFCC00", "fontStyle": "bold" } },
    { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#007AFF", "fontStyle": "bold" } },
    { "scope": ["heading.3.markdown"], "settings": { "foreground": "#FF3B30", "fontStyle": "bold" } },
    { "scope": ["heading.4.markdown"], "settings": { "foreground": "#34C759", "fontStyle": "bold" } },
    { "scope": ["heading.5.markdown"], "settings": { "foreground": "#AF52DE", "fontStyle": "bold" } },
