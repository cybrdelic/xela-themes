    { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#1C1D3A" } },
    { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["markup.table"], "settings": { "foreground": "#EDEAFB" } },
    { "scope": ["markup.table.header"], "settings": { "foreground": "#7AD0FF", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#1C1D3A" } },
    { "scope": ["markup.strikethrough"], "settings": { "foreground": "#B8B5CD", "fontStyle": "strikethrough" } },
    { "scope": ["markup.inserted"], "settings": { "foreground": "#63FFC9" } },
    { "scope": ["markup.deleted"], "settings": { "foreground": "#FF5A87" } },
    { "scope": ["markup.changed"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#F2E7B9" } },
    { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#C79BFF" } },
