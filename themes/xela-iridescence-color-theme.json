  "tokenColors": [
    { "scope": ["source","meta.brace","meta.delimiter","punctuation","punctuation.definition.parameters","punctuation.definition.block"], "settings": { "foreground": "#F7F6FE" } },
    { "scope": ["comment","punctuation.definition.comment"], "settings": { "foreground": "#8F91B0" } },
    { "scope": ["comment.block.documentation","comment.line.documentation"], "settings": { "foreground": "#9FA1C1" } },
