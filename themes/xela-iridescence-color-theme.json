    { "scope": ["variable.parameter","meta.parameter","meta.function.parameters"], "settings": { "foreground": "#F2E7B9" } },
    { "scope": ["variable.other.property","support.variable.property","meta.object-literal.key","meta.mapping.key","string.unquoted.label.js"], "settings": { "foreground": "#F7F6FE" } },
    { "scope": ["keyword.control.import","keyword.control.export","support.module","entity.name.namespace"], "settings": { "foreground": "#A26BFF" } },
    { "scope": ["support.class.component","entity.name.tag.tsx","entity.name.tag.jsx"], "settings": { "foreground": "#7AD0FF" } },
    { "scope": ["entity.name.tag.html","entity.name.tag"], "settings": { "foreground": "#FFD166" } },
