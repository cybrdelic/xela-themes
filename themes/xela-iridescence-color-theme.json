    { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#8F91B0" } },
    { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#AAA7BD", "fontStyle": "italic" } },
