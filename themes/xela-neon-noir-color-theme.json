{
  "$schema": "vscode://schemas/color-theme",
  "name": "XELA Neon Noir â€” Cyber Noir",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#F472B6AA",
    "selection.background": "#F472B633",

    "titleBar.activeBackground": "#0A0A0A",
    "titleBar.inactiveBackground": "#0A0A0A",
    "titleBar.activeForeground": "#F5F7FA",
    "titleBar.inactiveForeground": "#B6BAC6",
    "titleBar.border": "#141414",

    "activityBar.background": "#0A0A0A",
    "activityBar.foreground": "#F5F7FA",
    "activityBar.activeBorder": "#F472B6",
    "activityBar.border": "#141414",
    "activityBarBadge.background": "#F472B6",
    "activityBarBadge.foreground": "#1A0A13",

    "sideBar.background": "#0A0A0A",
    "sideBar.border": "#141414",

    "statusBar.background": "#0A0A0A",
    "statusBar.border": "#141414",

    "panel.background": "#0A0A0A",
    "panel.border": "#141414",

    "editorGroupHeader.tabsBackground": "#0A0A0A",
    "editorGroupHeader.tabsBorder": "#141414",

    "tab.activeBackground": "#0A0A0A",
    "tab.inactiveBackground": "#0A0A0A",
    "tab.border": "#0A0A0A",
    "tab.activeForeground": "#F5F7FA",
    "tab.inactiveForeground": "#B6BAC6",
    "tab.activeBorderTop": "#F472B6",

    "editor.background": "#0A0A0A",
    "editor.foreground": "#F5F7FA",
    "editor.lineHighlightBackground": "#111111",
    "editor.selectionBackground": "#F472B633",
    "editorCursor.foreground": "#F472B6",

    "editorHoverWidget.background": "#0F0F10",
    "editorHoverWidget.border": "#3F1F33",
    "editorWidget.background": "#0F0F10",
    "editorWidget.border": "#3F1F33",
    "widget.shadow": "#00000088",

    "list.activeSelectionBackground": "#121212",
    "list.hoverBackground": "#141414",
    "list.focusBackground": "#121212",

    "button.background": "#F472B6",
    "button.hoverBackground": "#FB8DCB",
    "button.foreground": "#1A0A13",

    "terminal.background": "#0A0A0A",
    "terminal.foreground": "#F5F7FA",
    "terminalCursor.foreground": "#F472B6",
    "terminal.ansiBlack": "#0A0A0A",
    "terminal.ansiRed": "#FB7185",
    "terminal.ansiGreen": "#34D399",
    "terminal.ansiYellow": "#FBBF24",
    "terminal.ansiBlue": "#93C5FD",
    "terminal.ansiMagenta": "#F472B6",
    "terminal.ansiCyan": "#22D3EE",
    "terminal.ansiWhite": "#E5E7EB",
    "terminal.ansiBrightBlack": "#4B5563",
    "terminal.ansiBrightRed": "#FCA5A5",
    "terminal.ansiBrightGreen": "#6EE7B7",
    "terminal.ansiBrightYellow": "#FDE68A",
    "terminal.ansiBrightBlue": "#BFDBFE",
    "terminal.ansiBrightMagenta": "#FB8DCB",
    "terminal.ansiBrightCyan": "#67E8F9",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["keyword"], "settings": { "foreground": "#F472B6", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#93C5FD" } },
    { "scope": ["string"], "settings": { "foreground": "#34D399" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#FBBF24" } },
  { "scope": ["comment"], "settings": { "foreground": "#9CA3AF", "fontStyle": "italic" } },
  { "scope": ["markup.heading","entity.name.section","punctuation.definition.heading.markdown"], "settings": { "fontStyle": "bold" } },
  { "scope": ["heading.1.markdown","markup.heading.setext.1.markdown"], "settings": { "foreground": "#F5F7FA", "fontStyle": "bold" } },
  { "scope": ["heading.2.markdown","markup.heading.setext.2.markdown"], "settings": { "foreground": "#F472B6", "fontStyle": "bold" } },
  { "scope": ["heading.3.markdown"], "settings": { "foreground": "#93C5FD", "fontStyle": "bold" } },
  { "scope": ["heading.4.markdown"], "settings": { "foreground": "#34D399", "fontStyle": "bold" } },
  { "scope": ["heading.5.markdown"], "settings": { "foreground": "#FBBF24", "fontStyle": "bold" } },
  { "scope": ["heading.6.markdown"], "settings": { "foreground": "#9CA3AF", "fontStyle": "bold" } },
  { "scope": ["markup.bold","markup.bold.markdown"], "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.bold.markdown"], "settings": { "foreground": "#F472B6" } },
  { "scope": ["markup.italic","markup.italic.markdown"], "settings": { "foreground": "#E5E7EB", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.italic.markdown"], "settings": { "foreground": "#9CA3AF" } },
  { "scope": ["markup.strikethrough","markup.strike","markup.deleted"], "settings": { "foreground": "#FB7185", "fontStyle": "strikethrough" } },
  { "scope": ["markup.underline.link","markup.underline.link.markdown","markup.link","meta.link","meta.link.inline","meta.link.reference"], "settings": { "foreground": "#22D3EE", "fontStyle": "underline" } },
  { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "#F5F7FA" } },
  { "scope": ["string.other.link.description.markdown"], "settings": { "foreground": "#93C5FD" } },
  { "scope": ["constant.other.reference.link.markdown"], "settings": { "foreground": "#9CA3AF" } },
  { "scope": ["markup.inline.raw","markup.raw.inline","markup.inline.code","markup.inline.raw.string.markdown","markup.raw.inline.markdown"], "settings": { "foreground": "#E9D5FF" } },
  { "scope": ["markup.fenced_code.block.markdown","markup.raw.block"], "settings": { "foreground": "#E9D5FF" } },
  { "scope": ["punctuation.definition.raw.markdown","punctuation.definition.fenced.markdown"], "settings": { "foreground": "#141414" } },
  { "scope": ["markup.quote","markup.quote.markdown"], "settings": { "foreground": "#9CA3AF", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.quote.begin.markdown"], "settings": { "foreground": "#F472B6" } },
  { "scope": ["meta.separator.markdown"], "settings": { "foreground": "#141414" } },
  { "scope": ["markup.list","markup.list.numbered","markup.list.unnumbered"], "settings": { "foreground": "#FBBF24" } },
  { "scope": ["punctuation.definition.list.begin.markdown"], "settings": { "foreground": "#FBBF24" } },
  { "scope": ["markup.table"], "settings": { "foreground": "#F5F7FA" } },
  { "scope": ["markup.table.header"], "settings": { "foreground": "#93C5FD", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.table.markdown","meta.table.separator.markdown"], "settings": { "foreground": "#141414" } },
  { "scope": ["meta.embedded.block.frontmatter","punctuation.definition.metadata.markdown"], "settings": { "foreground": "#9CA3AF" } },
  { "scope": ["markup.inserted"], "settings": { "foreground": "#34D399" } },
  { "scope": ["markup.deleted"], "settings": { "foreground": "#FB7185" } },
  { "scope": ["markup.changed"], "settings": { "foreground": "#FBBF24" } },
  { "scope": ["markup.input.checkbox"], "settings": { "foreground": "#22D3EE" } },
  { "scope": ["meta.image.inline","meta.image.reference"], "settings": { "foreground": "#93C5FD" } }
  ],
  "semanticTokenColors": {
    "function": "#93C5FD",
    "string": "#34D399",
    "number": "#FBBF24",
    "keyword": "#F472B6",
    "type": "#E9D5FF"
  }
}
